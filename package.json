{
  "name": "foodvault-distribution-api",
  "version": "0.0.1",
  "description": "Backend Server for FoodVault Distribution WebApp",
  "author": "Miguel Chevres",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prisma:prod:deploy": "npx prisma migrate deploy",
    "prisma:dev:deploy": "prisma migrate deploy dev",
    "db:dev:rm": "docker compose rm db -s -f -v",
    "db:dev:up": "docker compose up db -d",
    "db:dev:restart": "pnpm run db:dev:rm && pnpm run db:dev:up && sleep 1 && pnpm run prisma:dev:deploy",
    "prisma:test:deploy": "dotenv -e .env.test -- prisma migrate deploy dev",
    "db:test:rm": "docker compose rm test-db -s -f -v",
    "db:test:up": "docker compose up test-db -d",
    "db:test:restart": "pnpm run db:test:rm && pnpm run db:test:up && sleep 1 && pnpm run prisma:test:deploy",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "pnpm run db:test:restart && dotenv -e .env.test -- jest",
    "test:watch": "pnpm run db:test:restart && dotenv -e .env.test -- jest --watch",
    "test:cov": "pnpm run db:test:restart && dotenv -e .env.test -- jest --coverage",
    "make-badges": "istanbul-badges-readme --logo='jest' --badge-style='plastic'",
    "make-badges:ci": "pnpm run make-badges -- --ci",
    "test:debug": "pnpm run db:test:restart && dotenv -e .env.test -- node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "pnpm run db:test:restart && dotenv -e .env.test -- jest --no-cache --watch --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/class-validator": "^0.13.4",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@prisma/client": "^4.12.0",
    "argon2": "^0.30.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "jest-junit": "^15.0.0",
    "nest-access-control": "^2.2.0",
    "passport": "^0.6.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-linkedin-oauth2": "^2.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "webpack": "^5.0.0"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.3.5",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.18",
    "@types/passport-facebook": "^2.1.11",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-linkedin-oauth2": "^1.5.3",
    "@types/passport-local": "^1.0.35",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "dotenv-cli": "^7.1.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "istanbul-badges-readme": "^1.8.5",
    "jest": "29.3.1",
    "jsonwebtoken": "^9.0.0",
    "pactum": "^3.3.2",
    "pactum-matchers": "^1.1.4",
    "prettier": "^2.3.2",
    "prisma": "^4.11.0",
    "source-map-support": "^0.5.20",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4",
    "uuid": "^9.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/dist/",
      "/node_modules/",
      "<rootDir>/main.ts",
      "<rootDir>/app.module.ts",
      "<rootDir>/prisma/",
      "<rootDir>/prisma-client-exception/",
      "<rootDir>/common/guards/",
      "<rootDir>/common/decorators/",
      "<rootDir>/auth/strategies/",
      "<rootDir>/auth/types/",
      "<rootDir>/auth/auth.module.ts",
      "<rootDir>/users/users.module.ts",
      "<rootDir>/users/dto/index.ts",
      "<rootDir>/users/entities"
    ],
    "coveragePathIgnorePatterns": [
      "/dist/",
      "/node_modules/",
      "<rootDir>/main.ts",
      "<rootDir>/app.module.ts",
      "<rootDir>/prisma/",
      "<rootDir>/prisma-client-exception/",
      "<rootDir>/common/guards/",
      "<rootDir>/common/decorators/",
      "<rootDir>/auth/strategies/",
      "<rootDir>/auth/types/",
      "<rootDir>/auth/auth.module.ts",
      "<rootDir>/users/users.module.ts",
      "<rootDir>/users/dto/index.ts",
      "<rootDir>/users/entities",
      "<rootDir>/restaurants/restaurant.module.ts",
      "<rootDir>/restaurants/entities",
      "<rootDir>/restaurants/dto/index.ts"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "coverageReporters": [
      "json",
      "text",
      "json-summary",
      "lcov",
      "clover",
      "cobertura"
    ]
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
